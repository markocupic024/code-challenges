    "n = 2\n",
    "\n",
    "A1 = np.random.rand(n, n)\n",
    "\n",
    "v = np.random.rand(n)\n",
    "\n",
    "w = np.random.rand(n)\n",
    "\n",
    "print(np.dot(A1@v, w) == v.dot(A1@w)) # A is not symmetric\n",
    "\n",
    "A2 = A1.T@A1\n",
    "\n",
    "print(np.dot(A2@v, w) == v.dot(A2@w)) # A is symmetric\n",
