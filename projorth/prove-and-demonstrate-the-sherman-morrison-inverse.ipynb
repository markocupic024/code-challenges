    "a = np.random.rand(5)\n",
    "b = np.random.rand(5)\n",
    "\n",
    "A = np.eye(5) - np.outer(a,b)\n",
    "\n",
    "Ai = np.eye(5) + np.outer(a,b)/(1 - np.dot(a,b))\n",
    "\n",
    "print(np.round(A@Ai))"
   ]
