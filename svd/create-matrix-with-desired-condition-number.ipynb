   "source": [
    "k = 50\n",
    "\n",
    "m = 7\n",
    "\n",
    "n = 10\n",
    "\n",
    "sv = np.linspace(k, 1, np.min([m,n]))\n",
    "\n",
    "S = np.zeros((m,n))\n",
    "\n",
    "np.fill_diagonal(S, sv)\n",
    "\n",
    "U, _ = np.linalg.qr(np.random.rand(m,m))\n",
    "\n",
    "V, _ = np.linalg.qr(np.random.rand(n,n))\n",
    "\n",
    "A = U@S@V.T\n",
    "\n",
    "np.linalg.cond(A)"
   ]
