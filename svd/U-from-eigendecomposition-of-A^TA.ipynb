   "source": [
    "m = 3\n",
    "\n",
    "A = np.random.rand(m, 6)\n",
    "\n",
    "Us, Ss, Vs = np.linalg.svd(A)\n",
    "\n",
    "L, V = np.linalg.eig(A.T@A)\n",
    "\n",
    "U = np.zeros((m, m))\n",
    "\n",
    "for i in range(m):\n",
    "    U[:, i] = A@V[:, i]/np.sqrt(L[i])\n",
    "\n",
    "print(Us)\n",
    "\n",
    "U"
   ]
