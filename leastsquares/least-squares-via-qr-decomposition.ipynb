   "source": [
    "m = 10\n",
    "n = 3\n",
    "\n",
    "X = np.random.rand(m, n)\n",
    "\n",
    "y = np.random.rand(m, 1)\n",
    "\n",
    "Q,R = np.linalg.qr(X, 'complete')\n",
    "\n",
    "b = np.linalg.solve(R.T@R,(Q@R).T@y)\n",
    "\n",
    "b1 = np.linalg.lstsq(X, y, rcond=None)[0]\n",
    "\n",
    "print(b)\n",
    "\n",
    "print(b1)"
   ]
  }
