   "source": [
    "A = np.random.rand(4,4)\n",
    "\n",
    "D = np.diag(np.diag(A))\n",
    "\n",
    "print(D)\n",
    "\n",
    "eigenval = np.linalg.eig(D)[0]\n",
    "\n",
    "print(eigenval)\n",
    "\n",
    "TL = np.tril(A)\n",
    "\n",
    "eigenval = np.linalg.eig(TL)[0]\n",
    "\n",
    "print(eigenval)\n",
    "\n",
    "TU = np.triu(A)\n",
    "\n",
    "eigenval = np.linalg.eig(TU)[0]\n",
    "\n",
    "print(eigenval)"
   ]
  }
